---
import Nav from "../components/Nav.astro";
import MainHeader from "./MainHeader.astro";
import "../styles/global.css";

export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<MainHeader title={title} />
	<body>
		<Nav />
		<slot />
	</body>
</html>
